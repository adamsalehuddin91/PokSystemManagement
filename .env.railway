# Railway Environment Variables
# Copy these to Railway Dashboard > Variables

APP_NAME="PokSystemManagement"
APP_ENV=production
APP_DEBUG=false
APP_URL=https://your-app.up.railway.app
APP_KEY=

# Database (Supabase PostgreSQL - same as local)
DB_CONNECTION=pgsql
DB_HOST=aws-1-ap-southeast-1.pooler.supabase.com
DB_PORT=6543
DB_DATABASE=postgres
DB_USERNAME=
DB_PASSWORD=

# Session & Cache (use file for Railway free tier)
SESSION_DRIVER=file
CACHE_STORE=file
QUEUE_CONNECTION=sync

# Other
LOG_CHANNEL=stderr
BCRYPT_ROUNDS=12
FILESYSTEM_DISK=local

# Company
COMPANY_DOC_PREFIX=ZAS
